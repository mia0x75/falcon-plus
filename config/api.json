{
	"log": {
		"level": "info"
	},
	"databases": {
		"portal": {
			"addr": "%%MYSQL%%/portal?charset=utf8mb4&loc=Local&parseTime=true",
			"max_idle": 4,
			"max_connections": 8,
			"wait_timeout": 30
		},
		"graph": {
			"addr": "%%MYSQL%%/graph?charset=utf8mb4&loc=Local&parseTime=true",
			"max_idle": 4,
			"max_connections": 8,
			"wait_timeout": 30
		},
		"uic": {
			"addr": "%%MYSQL%%/uic?charset=utf8mb4&loc=Local&parseTime=true",
			"max_idle": 4,
			"max_connections": 8,
			"wait_timeout": 30
		},
		"dashboard": {
			"addr": "%%MYSQL%%/dashboard?charset=utf8mb4&loc=Local&parseTime=true",
			"max_idle": 4,
			"max_connections": 8,
			"wait_timeout": 30
		},
		"alarms": {
			"addr": "%%MYSQL%%/alarms?charset=utf8mb4&loc=Local&parseTime=true",
			"max_idle": 4,
			"max_connections": 8,
			"wait_timeout": 30
		}
	},
	"graphs": {
		"cluster": {
			"graph-00": "127.0.0.1:6070"
		},
		"max_connections": 100,
		"max_idle": 100,
		"connect_timeout": 1000,
		"execute_timeout": 5000,
		"replicas": 500
	},
	"metric_list_file": "./api/data/metric",
	"rpc": {
		"enabled": true,
		"listen": "0.0.0.0:6040"
	},
	"listen": "%%API_HTTP%%",
	"access_control": true,
	"signup_disable": false,
	"skip_auth": false,
	"default_token": "%%API_DEFAULT_TOKEN%%",
	"gen_doc": false,
	"gen_doc_path": "doc/module.html"
}
