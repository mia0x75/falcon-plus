{
	"debug": true,
	"http": {
		"enabled": true,
		"listen": "%%GATEWAY_HTTP%%"
	},
	"rpc": {
		"enabled": true,
		"listen": "%%GATEWAY_RPC%%"
	},
	"socket": {
		"enabled": true,
		"listen": "%%GATEWAY_SOCKET%%",
		"timeout": 3600
	},
	"transfer": {
		"enabled": true,
		"batch": 200,
		"retry": 1,
		"connect_timeout": 1000,
		"execute_timeout": 5000,
		"max_connections": 32,
		"max_idle": 32,
		"cluster": {
			"t1": "%%TRANSFER_RPC%%"
		}
	}
}
