{
	"log": {
		"level": "info"
	},
	"hostname": "",
	"ip": "",
	"plugin": {
		"enabled": false,
		"dir": "./plugins",
		"git": "https://github.com/open-falcon/plugin.git",
		"logs": "./logs"
	},
	"heartbeat": {
		"enabled": true,
		"addrs": [
			"%%HBS_RPC%%"
		],
		"interval": 60,
		"timeout": 1000
	},
	"transfer": {
		"enabled": true,
		"addrs": [
			"%%TRANSFER_RPC%%"
		],
		"interval": 60,
		"timeout": 1000
	},
	"http": {
		"enabled": true,
		"listen": "%%AGENT_HTTP%%",
		"backdoor": false,
		"root": "/usr/share/mfp/agent"
	},
	"collector": {
		"system": {
			"iface_prefix": [
				"eth",
				"em"
			],
			"mount_point": []
		}
	},
	"default_tags": {},
	"ignore": {
		"df.inodes.total": true,
		"df.inodes.free": true,
		"df.inodes.used": true,
		"df.inodes.used.percent": true
	}
}
