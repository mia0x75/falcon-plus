{
	"log": {
		"level": "info"
	},
	"http": {
		"enabled": true,
		"listen": "%%AGGREGATOR_HTTP%%"
	},
	"database": {
		"addr": "%%MYSQL%%/portal?loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8,
		"wait_timeout": 30,
		"ids": [1, -1],
		"interval": 55
	},
	"api": {
		"connect_timeout": 500,
		"request_timeout": 2000,
		"api": "http://%%API_HTTP%%",
		"token": "%%API_DEFAULT_TOKEN%%",
		"agent": "http://%%AGENT_HTTP%%/v1/push"
	}
}
