{
	"log": {
		"level": "info"
	},
	"http": {
		"enabled": true,
		"listen": "%%ALARM_HTTP%%"
	},
	"redis": {
		"addr": "%%REDIS%%",
		"password": "",
		"max_idle": 5,
		"wait_timeout": 240
	},
	"queue": {
		"high_queues": [
			"/q/event:p0",
			"/q/event:p1",
			"/q/event:p2"
		],
		"low_queues": [
			"/q/event:p3",
			"/q/event:p4",
			"/q/event:p5",
			"/q/event:p6"
		],
		"instant_queues": {
			"im": "/q/im",
			"sms": "/q/sms",
			"mail": "/q/mail"
		},
		"latent_queues": {
			"im": "/q/user/im",
			"sms": "/q/user/sms",
			"mail": "/q/user/mail"
		}
	},
	"api": {
		"im": "http://127.0.0.1:10086/wechat",
		"sms": "http://127.0.0.1:10086/sms",
		"mail": "http://127.0.0.1:10086/mail",
		"dashboard": "http://127.0.0.1:8081",
		"api": "http://%%API_HTTP%%",
		"token": "%%API_DEFAULT_TOKEN%%"
	},
	"database": {
		"addr": "%%MYSQL%%/alarms?charset=utf8mb4&loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8,
		"wait_timeout": 30
	},
	"worker": {
		"im": 10,
		"sms": 10,
		"mail": 50
	},
	"housekeeper": {
		"event_retention_days": 7,
		"event_delete_batch": 100
	}
}
