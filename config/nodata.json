{
	"log": {
		"level": "info"
	},
	"http": {
		"enabled": true,
		"listen": "%%NODATA_HTTP%%"
	},
	"api": {
		"connect_timeout": 500,
		"request_timeout": 2000,
		"addr": "http://%%API_HTTP%%",
		"token": "%%API_DEFAULT_TOKEN%%"
	},
	"database": {
		"enabled": true,
		"addr": "%%MYSQL%%/portal?loc=Local&parseTime=true&wait_timeout=604800",
		"max_idle": 4,
		"max_connections": 8
	},
	"collector": {
		"enabled": true,
		"batch": 200,
		"concurrent": 10
	},
	"sender": {
		"enabled": true,
		"connect_timeout": 500,
		"request_timeout": 2000,
		"transfer_addr": "%%TRANSFER_HTTP%%",
		"batch": 500
	}
}
