{
	"debug": true,
	"http": {
		"enabled": true,
		"listen": "%%NODATA_HTTP%%"
	},
	"plus_api": {
		"connect_timeout": 500,
		"request_timeout": 2000,
		"addr": "http://127.0.0.1:8080",
		"token": "default-token-used-in-server-side"
	},
	"config": {
		"enabled": true,
		"dsn": "%%MYSQL%%/portal?loc=Local&parseTime=true&wait_timeout=604800",
		"max_idle": 4
	},
	"collector": {
		"enabled": true,
		"batch": 200,
		"concurrent": 10
	},
	"sender": {
		"enabled": true,
		"connect_timeout": 500,
		"request_timeout": 2000,
		"transfer_addr": "%%TRANSFER_HTTP%%",
		"batch": 500
	}
}