{
	"log": {
		"level": "info"
	},
	"http": {
		"enabled": true,
		"listen": "%%EXPORTER_HTTP%%"
	},
	"index": {
		"enabled": false,
		"addr": "%%MYSQL%%/graph?charset=utf8&parseTime=True&loc=Local",
		"max_idle": 4,
		"max_connections": 8,
		"wait_timeout": 30,
		"auto_delete": false,
		"cluster": {
			"test.hostname01:6071": "0 0 0 ? * 0-5",
			"test.hostname02:6071": "0 30 0 ? * 0-5"
		}
	},
	"agent": {
		"enabled": false,
		"dsn": "",
		"max_idle": 4,
		"cleaner": {
			"interval": 60
		},
		"plugin": {
			"pattern": "",
			"interval": 60,
			"concurrent": 4,
			"connect_timeout": 500,
			"request_timeout": 500
		}
	},
	"collector": {
		"enabled": true,
		"agent": "http://%%AGENT_HTTP%%/v1/push",
		"pattern": "http://%s/statistics/all",
		"cluster": [
			"transfer,%%TRANSFER_HTTP%%",
			"graph,%%GRAPH_HTTP%%",
			"exporter,%%EXPORTER_HTTP%%"
		]
	},
	"monitor": {
		"enabled": true,
		"alarm": {
			"enabled": true,
			"url": ""
		},
		"pattern": "http://%s/health",
		"hosts": {
			"agents": [
				"%%AGENT_HTTP%%"
			],
			"modules": {
				"aggregator-00": "%%AGGREGATOR_HTTP%%",
				"alarm-00": "%%ALARM_HTTP%%",
				"api-00": "%%API_HTTP%%",
				"exporter-00": "%%EXPORTER_HTTP%%",
				"gateway-00": "%%GATEWAY_HTTP%%",
				"graph-00": "%%GRAPH_HTTP%%",
				"hbs-00": "%%HBS_HTTP%%",
				"judge-00": "%%JUDGE_HTTP%%",
				"nodata-00": "%%NODATA_HTTP%%",
				"transfer-00": "%%TRANSFER_HTTP%%",
				"updater-00": "%%UPDATER_HTTP%%"
			}
		}
	}
}
