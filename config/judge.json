{
	"log": {
		"level": "info"
	},
	"debug_host": "nil",
	"remain": 11,
	"http": {
		"enabled": true,
		"listen": "%%JUDGE_HTTP%%"
	},
	"rpc": {
		"enabled": true,
		"listen": "%%JUDGE_RPC%%"
	},
	"hbs": {
		"servers": [
			"%%HBS_HTTP%%"
		],
		"timeout": 300,
		"interval": 60
	},
	"alarm": {
		"min_interval": 120,
		"queue_pattern": "event:p%d",
		"redis": {
			"addr": "%%REDIS%%",
			"password": "",
			"max_idle": 5,
			"connect_timeout": 5,
			"read_timeout": 5,
			"write_timeout": 5,
			"wait_timeout": 240
		}
	}
}
