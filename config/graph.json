{
	"debug": false,
	"http": {
		"enabled": true,
		"listen": "%%GRAPH_HTTP%%"
	},
	"rpc": {
		"enabled": true,
		"listen": "%%GRAPH_RPC%%"
	},
	"rrd": {
		"storage": "./data/6070"
	},
	"db": {
		"dsn": "%%MYSQL%%/graph?loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8
	},
	"execute_timeout": 5000,
	"io_workers": 64,
	"migrate": {
		"enabled": false,
		"concurrency": 2,
		"replicas": 500,
		"cluster": {
			"graph-00": "127.0.0.1:6070"
		}
	}
}
