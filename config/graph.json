{
	"log": {
		"level": "info"
	},
	"http": {
		"enabled": true,
		"listen": "%%GRAPH_HTTP%%"
	},
	"rpc": {
		"enabled": true,
		"listen": "%%GRAPH_RPC%%"
	},
	"rrd": {
		"storage": "./data/6070"
	},
	"database": {
		"addr": "%%MYSQL%%/graph?charset=utf8mb4&loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8,
		"wait_timeout": 30
	},
	"execute_timeout": 5000,
	"io_workers": 64,
	"migrate": {
		"enabled": false,
		"concurrency": 2,
		"replicas": 500,
		"cluster": {
			"graph-00": "127.0.0.1:6070"
		}
	},
	"pfc": {
		"hostname": "127.0.0.1",
		"step": 5,
		"prefix": "graph",
		"tags": "",
		"bases": [
			"debug",
			"runtime"
		],
		"push": {
			"enabled": true,
			"api": "http://%%GATEWAY_HTTP%%/api/push"
		},
		"http": {
			"enabled": true,
			"listen": "0.0.0.0:2025"
		}
	}
}
