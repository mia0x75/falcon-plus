{
	"log": {
		"level": "debug"
	},
	"http": {
		"enabled": true,
		"listen": "0.0.0.0:6055"
	},
	"database": {
		"addr": "root:@tcp(127.0.0.1:3306)/portal?loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8,
		"ids": [
			1,
			-1
		],
		"interval": 55
	},
	"api": {
		"connect_timeout": 500,
		"request_timeout": 2000,
		"api": "http://127.0.0.1:8080",
		"token": "%%API_DEFAULT_TOKEN%%",
		"agent": "http://127.0.0.1:1988/v1/push"
	}
}