{
	"log": {
		"level": "debug"
	},
	"hostname": "",
	"ip": "",
	"plugin": {
		"enabled": false,
		"dir": "./plugin",
		"git": "https://github.com/open-falcon/plugin.git",
		"logs": "./logs"
	},
	"heartbeat": {
		"enabled": true,
		"addrs": [
			"127.0.0.1:6030"
		],
		"interval": 60,
		"timeout": 1000
	},
	"transfer": {
		"enabled": true,
		"addrs": [
			"127.0.0.1:8433",
			"127.0.0.1:8433"
		],
		"interval": 5,
		"timeout": 1000
	},
	"http": {
		"enabled": true,
		"listen": ":1988",
		"backdoor": false,
		"root": "/usr/share/mfp/agent"
	},
	"collector": {
		"system": {
			"iface_prefix": [
				"eth",
				"em"
			],
			"mount_point": []
		}
	},
	"default_tags": {},
	"ignore": {
		"df.inodes.total": true,
		"df.inodes.free": true,
		"df.inodes.used": true,
		"df.inodes.used.percent": true
	}
}