{
	"log": {
		"level": "debug"
	},
	"http": {
		"enabled": true,
		"listen": "0.0.0.0:6071"
	},
	"rpc": {
		"enabled": true,
		"listen": "0.0.0.0:6070"
	},
	"rrd": {
		"storage": "./data/6070"
	},
	"database": {
		"addr": "root:@tcp(127.0.0.1:3306)/graph?loc=Local&parseTime=true",
		"max_idle": 4,
		"max_connections": 8
	},
	"execute_timeout": 5000,
	"io_workers": 64,
	"migrate": {
		"enabled": false,
		"concurrency": 2,
		"replicas": 500,
		"cluster": {
			"graph-00" : "127.0.0.1:6070"
		}
	}
}
