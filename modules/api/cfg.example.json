		"port": 8089
	},
	"listen": ":8080",
	"access_control": true,
	"signup_disable": false,
	"skip_auth": false,
	"default_token": "default-token-used-in-server-side",
	"gen_doc": false,
	"gen_doc_path": "doc/module.html"
