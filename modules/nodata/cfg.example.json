{
	"log": {
		"level": "debug"
	},
	"http": {
		"enabled": true,
		"listen": "0.0.0.0:6090"
	},
	"api": {
		"connect_timeout": 500,
		"request_timeout": 2000,
		"addr": "http://127.0.0.1:8080",
		"token": "default-token-used-in-server-side"
	},
	"database": {
		"enabled": true,
		"addr": "root:@tcp(127.0.0.1:3306)/portal?loc=Local&parseTime=true&wait_timeout=604800",
		"max_idle": 4,
		"max_connections": 8,
		"wait_timeout": 30
	},
	"collector": {
		"enabled": true,
		"batch": 200,
		"concurrent": 10
	},
	"transfer": {
		"enabled": true,
		"connect_timeout": 500,
		"request_timeout": 2000,
		"addr": "127.0.0.1:6061",
		"batch": 500
	}
}